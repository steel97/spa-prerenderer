  "CacheCrawler": {
    "Puppeteer": {
      "BrowserSource": "local",
      "Headless": true
    },
    "RescanInterval": 600000,
    "PageScanTimeout": 10000,
    "PageScanWait": 3000,
    "CacheToMemory": true,
    "CacheToFS": true,
    "BaseUrl": "http://localhost:4999",
    "CacheRoutes": [
      {
        "Pattern": "/"
      },
      {
        "Pattern": "/404"
      },
      {
        "Pattern": "/{lang}"
      },
      {
        "Pattern": "/{lang}/faq"
      },
      {
        "Pattern": "/{lang}/tos"
      },
      {
        "Pattern": "/{lang}/privacy"
      },
      {
        "Pattern": "/{lang}/coin/{coin}"
      },
      {
        "Pattern": "/{lang}/calc"
      },
      {
        "Pattern": "/{lang}/calc/what-to-mine-gpu"
      },
      {
        "Pattern": "/{lang}/calc/what-to-mine-nvidia"
      },
      {
        "Pattern": "/{lang}/calc/what-to-mine-amd"
      },
      {
        "Pattern": "/{lang}/best-gpu-for-mining"
      },
      {
        "Pattern": "/{lang}/device/gpu/{gpu-link}"
      }
    ]
  },
  "Sitemap": {
    "UseSitemapGenerator": true,
    "SitemapUrlPattern": "/sitemap.xml",
    "BaseUrl": "https://woolypooly.com",
    "RescanConfigInterval": 600000,
    "Alternates": [
      {
        "Id": "langcoin",
        "TagName": "link",
        "WithVariants": true,
        "Props": [
          {
            "Name": "rel",
            "Value": "alternate"
          },
          {
            "Name": "hreflang",
            "Value": "{lang}"
          },
          {
            "Name": "href",
            "Value": "{__url_variant}"
          }
        ],
        "Variants": [
          "lang:current",
          "coin:top"
        ]
      },
      {
        "Id": "langgpu",
        "TagName": "link",
        "WithVariants": true,
        "Props": [
          {
            "Name": "rel",
            "Value": "alternate"
          },
          {
            "Name": "hreflang",
            "Value": "{lang}"
          },
          {
            "Name": "href",
            "Value": "{__url_variant}"
          }
        ],
        "Variants": [
          "lang:current",
          "gpu-link:top"
        ]
      }
    ],
    "Routes": [
      {
        "Pattern": "/",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": []
      },
      {
        "Pattern": "/{lang}",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/faq",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/tos",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/privacy",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/coin/{coin}",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/calc",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/calc/what-to-mine-gpu",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/calc/what-to-mine-nvidia",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/calc/what-to-mine-amd",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/best-gpu-for-mining",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langcoin"
        ]
      },
      {
        "Pattern": "/{lang}/device/gpu/{gpu-link}",
        "ChangeFrequency": "always",
        "Priority": 0.2,
        "WithAlternate": [
          "langgpu"
        ]
      }
    ]
  },
  "SPA": {
    "Redirect": {
      "IncludeQueryString": false,
      "RedirectRoutes": [
        {
          "Match": "^\/$",
          "Target": "/en"
        },
        {
          "Match": "^\/youtube\\/?$",
          "Target": "https://www.youtube.com/channel/UCfRUnJnNYU1YsrEn94KYtQA"
        }
      ]
    },
    "NotFound": {
      "Use404Code": true,
      "IncludeQueryString": false,
      "KnownRoutes": [
        {
          "Pattern": "^\/$"
        },
        {
          "Pattern": "^\/youtube\\/?$"
        },
        {
          "Pattern": "^\/discord\\/?$"
        },
        {
          "Pattern": "^\/telegram\\/?$"
        },
        {
          "Pattern": "^\/telegramru\\/?$"
        },
        {
          "Pattern": "^\/telegrames\\/?$"
        },
        {
          "Pattern": "^\/{lang}\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/faq\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/tos\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/privacy\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/coin\/{coin}\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/coin\/{coin}\/wallet\/.+$"
        },
        {
          "Pattern": "^\/{lang}\/calc\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/calc\/what-to-mine-gpu\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/calc\/what-to-mine-nvidia\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/calc\/what-to-mine-amd\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/best-gpu-for-mining\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/device\/gpu\/{gpu-link}\\/?$"
        }
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}