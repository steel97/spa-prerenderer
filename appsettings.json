{
  "Kestrel": {
    "EndPointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "HTTP": {
        "Url": "http://*:4999"
      }
    }
  },
  "AllowedHosts": "*",
  "Common": {
    "CacheHTML": true,
    "CacheHTMLInterval": 60000,
    "Placeholders": [
      {
        "Key": "lang",
        "Order": 0,
        "Targets": [
          "en",
          "fr",
          "de",
          "es",
          "it",
          "ja",
          "hu",
          "bg",
          "pt",
          "th",
          "vi",
          "pl",
          "ru",
          "ka",
          "cs",
          "sv",
          "zh",
          "tr",
          "hi",
          "fa",
          "id",
          "kr",
          "nl",
          "sr"
        ]
      },
      {
        "Key": "coin",
        "Order": 1,
        "Targets": [
          "eth",
          "etc",
          "rvn",
          "veil",
          "cfx",
          "erg",
          "ctxc",
          "ae",
          "aion",
          "sero",
          "grin",
          "mwc",
          "zel",
          "btg",
          "firo"
        ]
      }
    ]
  },
  "CacheCrawler": {
    "Puppeteer": {
      "BrowserSource": "local",
      "Headless": true
    },
    "RescanInterval": 600000,
    "PageScanTimeout": 10000,
    "PageScanWait": 1000,
    "CacheToMemory": true,
    "CacheToFS": true,
    "BaseUrl": "http://localhost:4999",
    "CacheRoutes": [
      {
        "Pattern": "/"
      },
      {
        "Pattern": "/404"
      },
      {
        "Pattern": "/{lang}"
      },
      {
        "Pattern": "/{lang}/faq"
      },
      {
        "Pattern": "/{lang}/tos"
      },
      {
        "Pattern": "/{lang}/privacy"
      },
      {
        "Pattern": "/{lang}/coin/{coin}"
      },
      {
        "Pattern": "/{lang}/calc"
      },
      {
        "Pattern": "/{lang}/calc/what-to-mine-nvidia"
      },
      {
        "Pattern": "/{lang}/calc/what-to-mine-amd"
      }
    ]
  },
  "SPA": {
    "Redirect": {
      "IncludeQueryString": false,
      "RedirectRoutes": [
        {
          "Match": "^\/$",
          "Target": "/en"
        },
        {
          "Match": "^\/youtube\\/?$",
          "Target": "https://www.youtube.com/channel/UCfRUnJnNYU1YsrEn94KYtQA"
        }
      ]
    },
    "NotFound": {
      "Use404Code": true,
      "IncludeQueryString": false,
      "KnownRoutes": [
        {
          "Pattern": "^\/$"
        },
        {
          "Pattern": "^\/discord\\/?$"
        },
        {
          "Pattern": "^\/telegram\\/?$"
        },
        {
          "Pattern": "^\/telegramru\\/?$"
        },
        {
          "Pattern": "^\/telegrames\\/?$"
        },
        {
          "Pattern": "^\/{lang}\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/faq\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/tos\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/privacy\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/coin\/{coin}\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/coin\/{coin}\/wallet\/.+$"
        },
        {
          "Pattern": "^\/{lang}\/calc\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/calc\/what-to-mine-nvidia\\/?$"
        },
        {
          "Pattern": "^\/{lang}\/calc\/what-to-mine-amd\\/?$"
        }
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}